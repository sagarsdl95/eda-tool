class papa;
  bit [3:0]len;
  bit [3:0] width;
  
endclass

class son extends papa;
  
  bit [6:0] area;
  function sqr;
   area = len * width;
       $display("area=%d",area); 
  endfunction
endclass

module top;
  son c;
 
  initial
    begin
     c=new();
      c.len=4;
       c.width=3;
      
     c.sqr();
      //$display("ram");
   
 end
  
endmodule
  
